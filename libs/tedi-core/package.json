{
  "name": "@tehik-ee/tedi-core",
  "version": "0.0.0-semantic-version",
  "main": "src/index.scss",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && cp -a src/* dist/ && cp README.md dist/ && cp package.json dist/",
    "release": "npx semantic-release -e semantic-release-monorepo --debug",
    "lint": "npx stylelint \"src/**/*.scss\" --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "lint-staged": "^15.3.0",
    "stylelint": "^16.13.2",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "*.{css,scss}": "stylelint \"**/*.{css,scss}\" --fix"
  }
}
