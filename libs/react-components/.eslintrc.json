{
  "extends": ["plugin:@nx/react-typescript", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    { "files": ["*.ts", "*.tsx", "*.js", "*.jsx"], "rules": {} },
    { "files": ["*.ts", "*.tsx"], "rules": {} },
    { "files": ["*.js", "*.jsx"], "rules": {} },
    {
      "files": ["*.stories.tsx"],
      "rules": {
        "react-hooks/rules-of-hooks": "off"
      }
    }
  ],
  "rules": {
    "@next/next/no-html-link-for-pages": 0,
    "@next/next/no-img-element": 0
  }
}
