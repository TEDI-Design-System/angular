{
  "name": "@tehik-ee/tedi-react",
  "version": "0.0.0-semantic-version",
  "type": "module",
  "main": "community.es.js",
  "module": "community.es.js",
  "exports": {
    "./tedi": {
      "types": "./tedi/index.d.ts",
      "import": "./tedi.es.js"
    },
    "./community": {
      "types": "./community/index.d.ts",
      "import": "./community.es.js"
    },
    "./index.css": "./index.css"
  },
  "files": [
    "tedi",
    "community",
    "index.css"
  ],
  "workspaces": [
    "../tedi-core"
  ],
  "scripts": {
    "copy:community": "cp -a dist/libs/react-components/src/community/. dist/community/",
    "copy:tedi": "cp -a dist/libs/react-components/src/tedi/. dist/tedi/",
    "start": "storybook dev -p 4400 -c .storybook",
    "build": "vite build --config vite.config.ts && npm run copy:community && npm run copy:tedi && rm -rf dist/libs && replace-in-file //fonts//g \"./fonts/\" dist/index.css --isRegex",
    "build:sb": "storybook build -c .storybook -o dist/storybook/react-components",
    "lint": "npx stylelint \"src/**/*.scss\" --fix && eslint --fix",
    "lint:ci": "npx stylelint \"src/**/*.scss\" && eslint",
    "test": "jest --config ./jest.config.ts --passWithNoTests",
    "test:coverage": "jest --config ./jest.config.ts --coverage",
    "release": "npx semantic-release --debug"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.26.9",
    "@mui/material": "^5.15.13",
    "@mui/x-date-pickers": "^5.0.20",
    "@tanstack/react-table": "^8.13.2",
    "@tehik-ee/tedi-core": "^1.0.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "draft-js": "^0.11.7",
    "draftjs-md-converter": "^1.5.2",
    "formik": "^2.4.5",
    "lodash-es": "^4.17.21",
    "next": "^14.1.3",
    "react": "18.3.1",
    "react-animate-height": "^3.2.3",
    "react-dom": "18.3.1",
    "react-select": "^5.8.0",
    "react-sticky-box": "2.0.5",
    "react-toastify": "^9.1.3",
    "what-input": "^5.2.12",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@avalane/storybook-addon-status": "^5.0.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@faker-js/faker": "^8.4.1",
    "@storybook/addon-a11y": "8.2.10",
